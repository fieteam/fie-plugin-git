{
  "extends": "eslint-config-ais/es6",
  "rules": {
    // 低版本 nodejs 需要在 strict 模式下支持 es6 语法
    "strict": "off",
    // 经常需要打印东西
    "no-console" : "off",
    // 不希望参数使用解构,低版本 nodejs 不支持
    "prefer-spread" : "off",
    // 会有很多地方需要直接使用 arguments
    "prefer-rest-params": "off",
    // 有些函数对外需要暴露 generator 类型函数,但里面确实只有同步代码
    "require-yield": "off"
  }
}
